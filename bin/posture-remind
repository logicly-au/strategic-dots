#!/bin/bash

# /usr/share/icons/gperfection2/48x48/stock/generic/stock_dialog-warning.png \
while true;
do
    notify-send -i \
    /usr/share/icons/Human/48x48/status/stock_dialog-warning.png \
        "Check your posture" \
        $"Shoulders back, hips back, chin-tuck, feet flat"

    sleep 600
done
